{
  "name": "13-day-stories",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "npm run web",
    "stop": "pkill -f 'expo start' || true",
    "clean": "rm -rf node_modules/.cache .expo && npm start",
    "build:android": "eas build --platform android --profile preview",
    "build": "npx expo export -p web",
    "convert:images": "node scripts/convert-images-to-webp.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.4.10",
    "expo": "~54.0.27",
    "expo-linear-gradient": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^54.0.4",
    "sharp": "^0.34.5"
  }
}
